{
  "name": "hogwarts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "add": "^2.0.6",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "current-device": "^0.7.2",
    "eslint-config-react-app": "^2.1.0",
    "js-cookie": "^2.2.0",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "socket.io-client": "^2.0.4"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start-com": "REACT_APP_WEBSITE=com react-scripts start",
    "start-cn": "REACT_APP_WEBSITE=cn react-scripts start",
    "build-com": "REACT_APP_WEBSITE=com react-scripts build",
    "build-cn": "REACT_APP_WEBSITE=cn react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "^6.0.0",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.3.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "source-map-explorer": "^1.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "html"
    ]
  }
}
